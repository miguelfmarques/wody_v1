<p id="notice"><%= notice %></p>

<h1>Athletes</h1>

<table>
  <thead>
    <tr>
      <th>Cpf</th>
      <th>Sex</th>
      <th>Birth date</th>
      <th>Phone</th>
      <th>Cell phone</th>
      <th>Street</th>
      <th>Number street</th>
      <th>Complement</th>
      <th>Neighborhood</th>
      <th>Cep</th>
      <th>City</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @athletes.each do |athlete| %>
      <tr>
        <td><%= athlete.email %></td>
        <td><%= athlete.login %></td>
        <td><%= athlete.password %></td>
        <td><%= athlete.name %></td>
        <td><%= athlete.cpf %></td>
        <td><%= athlete.sex %></td>
        <td><%= athlete.birth_date %></td>
        <td><%= athlete.phone %></td>
        <td><%= athlete.cell_phone %></td>
        <td><%= athlete.street %></td>
        <td><%= athlete.number_street %></td>
        <td><%= athlete.complement %></td>
        <td><%= athlete.neighborhood %></td>
        <td><%= athlete.cep %></td>
        <td><%= athlete.city %></td>
        <td><%= athlete.state %></td>
        <td><%= link_to 'Show', athlete %></td>
        <td><%= link_to 'Edit', edit_athlete_path(athlete) %></td>
        <td><%= link_to 'Destroy', athlete, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New athlete', new_athlete_path %>
